<sensor>
	<tracker>Microsoft-v2</tracker>
	<mode>Full Body</mode>
	<mirrormode>true</mirrormode>
	<smoothing>true</smoothing>
	<smoothingfactor>0.5</smoothingfactor>
	<correction>0.5</correction>
	<prediction>0.5</prediction>
	<jitter>0.05</jitter>
	<deviation>0.04</deviation>
</sensor>
<server>
	<transformations>Global coordinates</transformations>
	<automaticassignment>false</automaticassignment>
	<tracker0>1</tracker0>
	<tracker1>2</tracker1>
	<tracker2/>
	<tracker3/>
</server>
<display>
	<showviewerwindow>true</showviewerwindow>
	<background>Depth</background>
	<foreground>Depth</foreground>
	<showconsolewindow>true</showconsolewindow>
	<movechildwindows>false</movechildwindows>
	<savewindowlayout>false</savewindowlayout>
</display>
<gestures>
	<gesture name="Right Side" timeout="0" enabled="true" outputloop="false" outputtimeout="0">
		<input type="0">
			<descriptor>lean</descriptor>
			<descriptor>right</descriptor>
			<descriptor>at least</descriptor>
			<descriptor>5</descriptor>
			<descriptor>degrees</descriptor>
		</input>
		<output type="0">
			<descriptor>hold</descriptor>
			<descriptor>d</descriptor>
			<descriptor>until complete</descriptor>
			<descriptor>0</descriptor>
		</output>
	</gesture>
	<gesture name="Left Side" timeout="0" enabled="true" outputloop="false" outputtimeout="0">
		<input type="0">
			<descriptor>lean</descriptor>
			<descriptor>left</descriptor>
			<descriptor>at least</descriptor>
			<descriptor>5</descriptor>
			<descriptor>degrees</descriptor>
		</input>
		<output type="0">
			<descriptor>hold</descriptor>
			<descriptor>q</descriptor>
			<descriptor>until complete</descriptor>
			<descriptor>0</descriptor>
		</output>
	</gesture>
	<gesture name="back" timeout="0" enabled="true" outputloop="false" outputtimeout="0">
		<input type="0">
			<descriptor>lean</descriptor>
			<descriptor>backward</descriptor>
			<descriptor>at least</descriptor>
			<descriptor>5</descriptor>
			<descriptor>degrees</descriptor>
		</input>
		<output type="0">
			<descriptor>hold</descriptor>
			<descriptor>s</descriptor>
			<descriptor>until complete</descriptor>
			<descriptor>0</descriptor>
		</output>
	</gesture>
	<gesture name="before" timeout="0" enabled="true" outputloop="false" outputtimeout="0">
		<input type="1">
			<descriptor>left foot</descriptor>
			<descriptor>in front of</descriptor>
			<descriptor>torso</descriptor>
			<descriptor>at least</descriptor>
			<descriptor>25</descriptor>
			<descriptor>centimeters</descriptor>
		</input>
		<output type="0">
			<descriptor>hold</descriptor>
			<descriptor>z</descriptor>
			<descriptor>until complete</descriptor>
			<descriptor>1</descriptor>
		</output>
	</gesture>
	<gesture name="Jump" timeout="0" enabled="true" outputloop="false" outputtimeout="0">
		<input type="0">
			<descriptor>jump</descriptor>
			<descriptor/>
			<descriptor>at least</descriptor>
			<descriptor>10</descriptor>
			<descriptor>centimeters</descriptor>
		</input>
		<output type="0">
			<descriptor>press</descriptor>
			<descriptor>space</descriptor>
			<descriptor>until complete</descriptor>
			<descriptor>0</descriptor>
		</output>
	</gesture>
	<gesture name="Crouch" timeout="0" enabled="true" outputloop="false" outputtimeout="0">
		<input type="2">
			<descriptor>left leg</descriptor>
			<descriptor>at least</descriptor>
			<descriptor>90</descriptor>
			<descriptor>degrees</descriptor>
		</input>
		<output type="0">
			<descriptor>hold</descriptor>
			<descriptor>left_control</descriptor>
			<descriptor>until complete</descriptor>
			<descriptor>0</descriptor>
		</output>
	</gesture>
	<gesture name="sprint" timeout="0" enabled="true" outputloop="false" outputtimeout="0">
		<input type="1">
			<descriptor>right hand</descriptor>
			<descriptor>behind</descriptor>
			<descriptor>right knee</descriptor>
			<descriptor>at least</descriptor>
			<descriptor>15</descriptor>
			<descriptor>centimeters</descriptor>
		</input>
		<input type="1">
			<descriptor>left hand</descriptor>
			<descriptor>behind</descriptor>
			<descriptor>left knee</descriptor>
			<descriptor>at least</descriptor>
			<descriptor>15</descriptor>
			<descriptor>centimeters</descriptor>
		</input>
		<output type="0">
